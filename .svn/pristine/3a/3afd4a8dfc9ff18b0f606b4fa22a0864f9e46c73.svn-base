#include "SubsetSum.hpp"
#include "DPMatrix.hpp"

namespace ss {


// S should be sorted ascending before calling this
size_t generateSetsDP(const uint64_t S[MAXN], const int n, const uint64_t sum, const uint64_t lower, const uint64_t upper,
                    vector<Bitset> &allSets) {
  vector<ss::Bitset> bitsets;
  DPMatrix matrix(S,n,S[0],upper);
  return matrix.generateSubsets(allSets,lower,upper);
}
} // end namespace

